/* pages/index/index.wxss */
/* 容器样式*/
page{
  width:100%;
  height:100%;
}
.indexContainer{
  width:100%;
  height:100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: #02a774;
}
.btn{
  width:200rpx;
  height :200rpx;
  border-radius: 50%;
  margin-top: 100rpx;
  text-align: center;
  line-height: 200rpx;
}
/* 头像样式 */
.avatar{
  width:200rpx;
  height:200rpx;
  border-radius: 50%;
  margin-top: 100rpx;
}
/* 欢迎用户的文本样式 */
.userName{
  font-weight: 700;
  font-size: 32rpx;
  margin: 200rpx 0 100rpx;
}
/* 按钮的样式 */
.button-container{
  width:200rpx;
  height:80rpx;
  border:1rpx solid #666;
  line-height: 80rpx;
  text-align: center;
  border-radius: 10rpx;
}
.button-container text{
  font-size:26rpx;
}